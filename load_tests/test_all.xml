<tsung loglevel="notice" version="1.0">

  <clients>
    <client host="localhost" use_controller_vm="true" maxusers="15000"/>
  </clients>

  <servers>
    <server host="54.173.135.77" port="80" type="tcp"/>
  </servers>


  <load>
    <arrivalphase phase="1" duration="30" unit="second">
      <users interarrival="2" unit="second"/>
    </arrivalphase>
    <arrivalphase phase="2" duration="30" unit="second">
      <users interarrival="1" unit="second"/>
    </arrivalphase>
    <arrivalphase phase="3" duration="30" unit="second">
      <users interarrival="0.5" unit="second"/>
    </arrivalphase>
  </load>

  <options>
    <option name="global_ack_timeout" value="2000"/>
    <option type="ts_http" name="user_agent">
      <user_agent probability="100">Mozilla/5.0 (Windows; U; Windows NT 5.2; fr-FR; rv:1.7.8) Gecko/20050511 Firefox/1.0.4</user_agent>
    </option>
  </options>

  <sessions>

    <session name="sign-up" probability="10" type="ts_http">

<!--home page-->
      <request>
        <http url="/" version="1.1" method="GET"/>
      </request>
     <thinktime value="2" random="true"/>

<!--search driver-->
      <request>
        <dyn_variable name="authenticity_token"/>
        <http url="/drivers" version="1.1" method="GET"/>
      </request>

      <thinktime value="2" random="true"/>

      <request subst="true">
       <http url="/drivers/index" 
  method="POST" version="1.1" 
  contents="utf8=%E2%9C%93&amp;
  authenticity_token=%%_authenticity_token%%&amp;
  search%5Bdeparture%5D=Santa+Barbara%2C+CA%2C+United+States&amp;
  search%5Bdestination%5D=Arizona%2C+United+States&amp;
  search%5Bdepart_time%5D=12%3A30&amp;
  search%5Bestimated_arrival_time%5D=20%3A30&amp;
  commit=Search">
       </http>
      </request>

      <thinktime value="2" random="true"/>

<!-- randomly choose a driver-->
       <setdynvars sourcetype="random_number" start="1" end="300">
        <var name="random_driver" />
      </setdynvars>

<!--  show detail of this driver-->
      <request  subst="true">
        <http url='/drivers/%%_random_driver%%' version="1.1" method="GET"/>
      </request>

<!--new user sign up-->
      <request>
        <http url="/signup" version="1.1" method="GET"/>
      </request>

     <thinktime value="2" random="true"/>

       <setdynvars sourcetype="random_number" start="1000" end="3000">
        <var name="random_user_num" />
      </setdynvars>

      <request subst="true">
        <http url="/users" method="POST" version="1.1" 
  contents="utf8=%E2%9C%93&amp;
  user%5Bname%5D=new_user_%%_random_user_num%%&amp;
  user%5Bemail%5D=new_user_%%_random_user_num%%%40example.com&amp;
  user%5Bpassword%5D=111111&amp;
  user%5Bpassword_confirmation%5D=111111&amp;
  commit=Create+my+account" 
  content_type="application/x-www-form-urlencoded">
      </http>
      </request>

     <thinktime value="2" random="true"/>

<!--book the driver (id = random_driver) -->
      <request>
        <http url="/drivers" version="1.1" method="GET"/>
      </request>

     <thinktime value="2" random="true"/>

       <request subst="true">
        <http url='/rides?driver_id=%%_random_driver%%' 
    method="POST" version="1.1" 
    contents="utf8=%E2%9C%93&amp;
    my_input=1&amp;commit=Book+a+Ride" 
    content_type="application/x-www-form-urlencoded">
        </http>
      </request>


    </session>
<!-- end of session "sign-up" -->

